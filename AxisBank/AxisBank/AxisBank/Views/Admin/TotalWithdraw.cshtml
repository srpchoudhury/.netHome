@model  List<tblAccountStatement>
@{
    ViewBag.Title = "TotalWithdraw";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Total Withdraw</h2>

<hr />
<table class=" table table-striped">
    <tr>
        <th>Date</th>
        <th>Description</th>
        <th>Debit</th>
        <th>Balance</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Date</td>
            <td>@item.Description</td>
            <td>@item.Debit</td>
            <td>@item.Balance</td>
        </tr>
    }
</table>


